{
  "id": "diploma",
  "displayName": "Diploma Form",
  "description": "A form to register a new degree/diploma",
  "jpeg": "http://i.imgur.com/ds4sVea.jpg",
  "schema": {
    "$schema": "http://json-schema.org/draft-04/schema#",
    "description": "",
    "type": "object",
    "properties": {
      "Soldiers Details": {
        "type": "object",
        "properties": {
          "Personal Info": {
            "type": "object",
            "properties": {
              "Personal Number": {
                "type": "string",
                "minLength": 6,
                "maxLength": 9
              },
              "Rank": {
                "type": "string",
                "enum": [
                  "Private",
                  "Corporal",
                  "Sergeant",
                  "Staff Sergeant",
                  "S.Lieutenant",
                  "Lieutenant",
                  "Captain",
                  "Major",
                  "Lieutenant Colonel",
                  "Colonel",
                  "Brigadier-general"
                ]
              },
              "First Name": {
                "type": "string",
                "minLength": 1
              },
              "Family Name": {
                "type": "string",
                "minLength": 3,
                "maxLength": 45
              }
            },
            "required": [
              "Personal Number",
              "Rank",
              "First Name",
              "Family Name"
            ]
          }
        },
        "required": [
          "Personal Info"
        ]
      },
      "Diploma Details": {
        "type": "object",
        "properties": {
          "Diploma Time": {
            "type": "string",
            "enum": [
              "Studied During his service",
              "Studied before permanent service"
            ]
          },
          "Diploma Start Date": {
            "type": "string",
            "format": "date-time"
          }
        },
        "required": [
          "Diploma Time",
          "Diploma Start Date"
        ]
      }
    },
    "required": [
      "Soldiers Details",
      "Diploma Details"
    ]
  },
  "next_steps": []
}